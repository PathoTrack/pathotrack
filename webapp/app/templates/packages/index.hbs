<div class="content">

	<div class="max-width-1170 clearfix section-header-wrap margin-top-0">
	  <h5 class="uppercase regular pull-left margin-0">Holidays</h5>
	</div>

	<div class="row bg-brand clearfix">
	  <div class="row title-row max-width-1170 clearfix">
	    <div class="col col-40">User</div>
	    <div class="col col-30">This month</div>
	    <div class="col col-30">This year</div>
	  </div>
	</div>

	<div class="max-width-1170">
	  	{{#each model as |leave|}}
		    <div class="row content-row clearfix">
		    	<div class="col col-40 col-center">
		    		{{leave.user_name}}
		    	</div>
		    </div>
	  	{{/each}}
	</div>

</div>
